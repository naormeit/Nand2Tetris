/**
 * Computes the sum of three bits.
 */
CHIP FullAdder {
    IN a, b, c;
    OUT sum, carry;

    PARTS:
    HalfAdder(a=a, b=b, sum=haSum, carry=haCarry);
    HalfAdder(a=haSum, b=c, sum=sum, carry=haCarry2);
    Or(a=haCarry, b=haCarry2, out=carry);
}